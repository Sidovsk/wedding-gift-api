{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "target": "esnext",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "sourceMap": true,
    "rootDir": "./src",
    "moduleResolution": "node",
    "baseUrl": ".",
    "resolveJsonModule": true,
    "checkJs": false,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
  },
  "typeRoots": [
    "./node_modules/@types",
    "@types"
  ],
  "include": [
    "src",
  ]
}
